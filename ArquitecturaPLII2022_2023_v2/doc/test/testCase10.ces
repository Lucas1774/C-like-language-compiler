
/* 

// Flujo combinadas (si para) y bloques
*/

vacio principal () {

	entero a;
	entero b;
	entero uno;

	a=0;
	escribe("a = 0?: ");
	escribeEnt(a);
	escribe();

	b=5;
	escribe("b = 5?: ");
	escribeEnt(b);
	escribe();
	
	uno=1;

	escribe("uno = 1?: ");
	escribeEnt(uno);
	escribe();

	mientras (a<5) {

		si (b<7) {
			escribe("b < 7: ");
			escribeEnt(b);
			escribe();
		}

		escribe("a < 5?: ");
		escribeEnt(a);
		escribe();	
		b = b + 1;
		a++;
	}

	/* bloque */
	{
		entero c;
		{
			c=1;
			escribe("c = 1 ?:");
			escribeEnt(c);
			escribe();
		}
	}
	devuelve;

}