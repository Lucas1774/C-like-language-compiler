#constante VERDADERO 1;
#constante FALSO 0;
#constante TAM 7;

tipo entero vector [TAM];

entero suma;
entero mayorDiez;

vacio principal () {
    entero a, b;
    a = 3;
    b = 5;
    suma = a + b; 
    mayorDiez =  suma < 10;
    si (mayorDiez == VERDADERO) {
        escribe ("mayor");
    } sino {
        escribe ("menor");
    }
    devuelve;
}