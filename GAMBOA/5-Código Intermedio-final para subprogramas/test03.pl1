# Ejemplo generacion codigo intermedio y final invocando un subprograma

programa test:
  variables x,z : entero;
  subprogramas
  funcion incrementar (VAR a:entero; b:entero): entero:
  	variables j: entero;
 	comienzo
		escribir("b(1) ");
		escribir(b);
		escribir();
	    j = a + b;
		a = j + 133;
		devolver j;
 	fin; 	
  
 comienzo
    z = 66;	
	x = 1;
	x = incrementar(z,x);
    escribir("x(67)= ");
	escribir(x);
	escribir();
	
	escribir("z(200)= ");
	escribir(z);
	escribir();
 fin.
