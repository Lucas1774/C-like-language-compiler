# Ejemplo generacion codigo intermedio y final - Referencias no locales

programa testReferenciasNoLocales:
  variables a1,b1,suma : entero;
  subprogramas
  procedimiento Sumar (VAR a:entero; VAR b:entero):
	variables s : entero;
	subprogramas
	procedimiento imprimir():
		comienzo
			escribir("Suma(5):");
			escribir(s);
		fin; 
  	
	comienzo
		s = a+b;
		imprimir();
 	fin; 	
  
 comienzo
    a1 = 2;
	b1 = 3;
    Sumar(a1,b1);
 fin.
